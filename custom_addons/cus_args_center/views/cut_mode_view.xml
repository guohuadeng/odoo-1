<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--征免性质-->
        <record id="cus_args_cut_mode_action" model="ir.actions.act_window">
            <field name="name">Cut Mode Code Table</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cus_args.cut_mode</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="cus_args_cut_mode_tree_view" model="ir.ui.view">
            <field name="name">cus_args.cut_mode.tree</field>
            <field name="model">cus_args.cut_mode</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="code"/>
                    <field name="name_cn"/>
                </tree>
            </field>
        </record>

        <record id="cus_args_cut_mode_search_view" model="ir.ui.view">
           <field name="name">cus_args.cut_mode.search</field>
           <field name="model">cus_args.cut_mode</field>
            <field name="arch" type="xml">
                <search>
                    <field name="code"/>
                    <field name="name_cn"/>
                </search>
           </field>
        </record>

        <record id="cus_args_cut_mode_form" model="ir.ui.view">
            <field name="name">cus_args.cut_mode.form</field>
            <field name="model">cus_args.cut_mode</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="code"/>
                            </group>
                            <group>
                                <field name="name_cn"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>



    </data>
</odoo>