<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="delegation_dec_license_doc_type" model="ir.actions.act_window">
            <field name="name">Customs DecLicenseDoc Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">basedata.dec_license_doc_type</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="dec_license_doc_type_form" model="ir.ui.view">
            <field name="name">Customs DecLicenseDoc Type form</field>
            <field name="model">basedata.dec_license_doc_type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="Code" string="Filing Bill Type Code"/>
                            </group>
                            <group>
                                <field name="NameCN" string="Chinese Name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="dec_license_doc_type_tree" model="ir.ui.view">
            <field name="name">Customs DecLicenseDoc Type tree</field>
            <field name="model">basedata.dec_license_doc_type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Code" string="Filing Bill Type Code"/>
                    <field name="NameCN" string="Chinese Name"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>