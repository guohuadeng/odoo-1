<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="customs_filing_bill_type_action" model="ir.actions.act_window">
            <field name="name">Customs Filing Bill Type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cus_args.filing_bill_type</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="filing_bill_type_form" model="ir.ui.view">
            <field name="name">Customs Filing Bill Type form</field>
            <field name="model">cus_args.filing_bill_type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="Code" string="Filing Bill Type Code"/>
                            </group>
                            <group>
                                <field name="NameCN" string="Chinese Name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="filing_bill_type_tree" model="ir.ui.view">
            <field name="name">Customs Filing Bill Type tree</field>
            <field name="model">cus_args.filing_bill_type</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Code" string="Filing Bill Type Code"/>
                    <field name="NameCN" string="Chinese Name"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>