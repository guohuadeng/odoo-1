<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="register_company_action" model="ir.actions.act_window">
            <field name="name">Register Company</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cus_args.register_company</field>
            <field name="view_mode">tree</field>
        </record>

        <record id="view_register_company_list" model="ir.ui.view">
            <field name="name">register company list view</field>
            <field name="model">cus_args.register_company</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="register_code"/>
                    <field name="unified_social_credit_code"/>
                    <field name="register_name_cn"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>