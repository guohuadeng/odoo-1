<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_customs_packing_form" model="ir.ui.view">
        <field name="name">packing form</field>
        <field name="model">cus_args.packing</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="NameCN"/>
                        </group>
                        <group>
                            <field name="Code"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_customs_packing_list" model="ir.ui.view">
        <field name="name">packing list</field>
        <field name="model">cus_args.packing</field>
        <field name="arch" type="xml">
            <tree>
                <field name="id"/>
                <field name="NameCN"/>
                <field name="Code"/>
            </tree>
        </field>
    </record>

    <act_window id="customs_packing_action"
                name="Pack"
                res_model="cus_args.packing"
                view_mode="tree,form"/>
</odoo>