<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="customs_cut_mode_action" model="ir.actions.act_window">
            <field name="name">customs cut mode</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cus_args.cut_mode</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="cus_cut_mode_form" model="ir.ui.view">
            <field name="name">customs cut mode form</field>
            <field name="model">cus_args.cut_mode</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="Code" string="Cut Mode Code"/>
                            </group>
                            <group>
                                <field name="NameCN" string="Chinese Name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="cus_cut_mode_tree" model="ir.ui.view">
            <field name="name">customs cut mode tree</field>
            <field name="model">cus_args.cut_mode</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Code" string="Cut Mode Code"/>
                    <field name="NameCN" string="Chinese Name"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>