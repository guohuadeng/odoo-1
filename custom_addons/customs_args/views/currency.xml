<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="customs_currency" model="ir.actions.act_window">
            <field name="name">Customs Currency</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">cus_args.cus_currency</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="cus_currency_form" model="ir.ui.view">
            <field name="name">Customs Currency form</field>
            <field name="model">cus_args.cus_currency</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group col="3" colspan="1">
                            <group>
                                <field name="Code" string="Currency Code"/>
                            </group>
                            <group>
                                <field name="symbol" string="Symbol"/>
                            </group>
                            <group>
                                <field name="NameCN" string="Chinese Name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="cus_currency_tree" model="ir.ui.view">
            <field name="name">Customs Currency tree</field>
            <field name="model">cus_args.cus_currency</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Code" string="Currency Code"/>
                    <field name="symbol" string="Symbol"/>
                    <field name="NameCN" string="Chinese Name"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>