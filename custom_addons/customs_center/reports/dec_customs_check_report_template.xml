<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="customs_center.report_dec_customs_check_template">
            <t t-call="report.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="report.external_layout">
                        <h2>check report</h2>
                    </t>
                </t>

            </t>
        </template>

        <template id="customs_center.print_dec_customs_invoice_template">
            <t t-call="report.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="report.external_layout">
                        <div class="page">
                            <h2>Report title</h2>
                            <p>This object's name is <span t-field="o.name"/></p>
                        </div>
                    </t>
                </t>

            </t>
        </template>

    </data>
</odoo>