<?xml version="1.0" encoding="UTF-8"?>
<templates >
    <t t-name="declare_element_modal">
        <div tabindex="-1" class="modal fade" role="dialog" data-backdrop="static">
            <div class="modal-dialog modal-lg">
                <div class="modal-content">
                    <div class="modal-header">
                        <button class="close" data-dismiss="modal">x</button>
                        <h4 class="modal-title">商品规范要素</h4>
                    </div>
                    <div class="modal-body">
                        <div class="o_view_manager_content">
                            <div class="o_form_view o_form_editable oe_form_dirty">
                                <div class="o_form_sheet">
                                    <div class="o_form_sheet" id="declare-element-names">


                                    </div>
                                    <div class=" o_form_sheet">
                                        <div class="row" style="padding-bottom: 20px">
                                            <div class="col-md-2"><label>规格型号</label><p name="goods_model_preview_count"></p></div>
                                            <div class="col-md-8"><p name="goods_model_preview"></p></div>
                                        </div>


                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <footer>
                            <button type="button" class="btn btn-sm oe_highlight">
                                <span>确定</span>
                            </button>
                            <button type="button" class="btn btn-sm" data-dismiss="modal">
                                <span>取消</span>
                            </button>
                        </footer>
                    </div>
            	</div>
            </div>
        </div>
    </t>
</templates>