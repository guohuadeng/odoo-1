<?xml version="1.0" encoding="UTF-8"?>
<templates >
   <t t-name="Prjects_dashboard">
      <div class="projects-container">
         <a id="projects_selectbox"></a>
         <!--<select id="projects_selectbox">-->
            <!--<option value="-1">-&#45;&#45; All Projects -&#45;&#45;</option>-->
         <!--</select>-->
      </div>
      <div class="clearfix"></div>
      <div class="">
         <div class="col-md-12 col-lg-12 col-sm-12 first-client-block">
            <div class="white-box">
               <div class="row-in">
                  <div class="col-lg-3 col-sm-6 row-in-br">
                     <div class="col-in">
                        <h3 class="box-title">已申报</h3>
                        <ul class="list-inline two-part">
                           <li><i class="fa fa-tasks text-warning"></i></li>
                           <li class="text-right"><span id="get_cus_send_count" class="counter total_clients">0</span></li>
                        </ul>
                     </div>
                  </div>
                  <div class="col-lg-3 col-sm-6 row-in-br  b-r-none">
                     <div class="col-in">
                        <h3 class="box-title">暂存成功</h3>
                        <ul class="list-inline two-part">
                           <li><i class="icon-layers text-inverse"></i></li>
                           <li class="text-right"><span id="get_cus_save_count" class="counter total_employees">0</span></li>
                        </ul>
                     </div>
                  </div>
                  <div class="col-lg-3 col-sm-6  row-in-br">
                     <div class="col-in">
                        <h3 class="box-title">申报成功</h3>
                        <ul class="list-inline two-part">
                           <li><i class="fa fa-check-square-o text-success"></i></li>
                           <li class="text-right"><span id="get_cus_dec_success" class="counter total_projects">0</span></li>
                        </ul>
                     </div>
                  </div>
                  <div class="col-lg-3 col-sm-6 b-0">
                     <div class="col-in">
                        <h3 class="box-title">状态异常</h3>
                        <ul class="list-inline two-part">
                           <li><i class="fa fa-exclamation-triangle text-danger"></i></li>
                           <li class="text-right"><span id="get_cus_dec_abnor" class="counter total_paid_invoice">0</span></li>
                        </ul>
                     </div>
                  </div>
               </div>
               <div class="clearfix"></div>
            </div>
         </div>
      </div>

      <div class="">
         <div class="col-md-6">
            <div class="white-box">
               <div class="col-xs-12">
                  <div class="">
                     <!--<h3 class="box-title">当天申报记录</h3>-->
                     <div class="pull-right">
                        <a href="#" id="get_chart_project_task" class="chart-tasks-link active">月</a> |
                        <a href="#" id="get_chart_project_task" class="chart-tasks-link">周</a> |
                        <a href="#" id="get_chart_project_task" class="chart-tasks-link">天</a> |
                     </div>
                     <div id="chart_employee_tasks"/>
                  </div>
               </div>
            <div class="clearfix"></div>
            </div>
         </div>
         <div class="col-md-3">
            <div class="white-box">
               <div class="col-xs-12">
                  <div class="">
                     <!--<h3 class="box-title">当天申报记录</h3>-->
                     <div class="pull-right">
                        <a href="#" id="get_chart_employee_task_2" class="chart-tasks-link active">报关单</a> |
                        <a href="#" id="get_chart_employee_task_2_sync" class="chart-tasks-link">协同报关单</a>
                     </div>
                     <div id="chart_employee_tasks_2"/>
                  </div>
               </div>
               <div class="clearfix"></div>
            </div>
         </div>
         <div class="col-md-3">
            <div class="white-box">
               <div class="col-xs-12">
                  <div class="">
                     <!--<h3 class="box-title">当天申报记录</h3>-->
                     <!--chart-issues-link-1-->
                     <div class="pull-right">
                        <a href="#" id="get_chart_employee_task_3" class="chart-tasks-link active">报关单</a> |
                        <a href="#" id="get_chart_employee_task_3_sync" class="chart-tasks-link">协同报关单</a>
                     </div>
                     <div id="chart_employee_tasks_3"/>
                  </div>
               </div>
               <div class="clearfix"></div>
            </div>
         </div>
         <div class="clearfix"></div>
      </div>

      <div class="">
         <div class="col-md-6">
            <div class="panel panel-default">
               <div class="panel-heading">Overdue Tasks</div>
               <div class="panel-wrapper collapse in">
                  <div class="panel-body overdue-tasks-container">

                  </div>
               </div>
            </div>
         </div>
         <div class="col-md-6">
            <div class="panel panel-default">
               <div class="panel-heading">Pending Issues</div>
               <div class="panel-wrapper collapse in">
                  <div class="panel-body pending-issue-container">

                  </div>
               </div>
            </div>
         </div>
         <div class="clearfix"></div>
      </div>
   </t>
</templates>